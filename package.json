{
  "name": "caralui",
  "version": "0.2.0",
  "type": "module",
  "types": "./dist/Caral.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/Caral.umd.cjs",
  "module": "./dist/Caral.js",
  "exports": {
    ".": {
      "import": "./dist/Caral.js",
      "require": "./dist/Caral.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_19061466bcf9a47",
    "commit": "cz"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "iconcaral2": "^0.0.17",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.1.3",
    "react-day-picker": "^8.10.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.0.2"
  },
  "peerDependencies": {
    "@types/node": "^22.12.0",
    "sass": "^1.83.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "devDependencies": {
    "@arcblock/addon-storysource": "^2.12.46",
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/js": "^9.13.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-storysource": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "@whitespace/storybook-addon-html": "^6.1.1",
    "@xfinx/storybook-addon-html": "^5.1.11",
    "autoprefixer": "^10.4.20",
    "chromatic": "^11.25.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.11.0",
    "playwright": "^1.51.0",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-release": "^24.2.3",
    "storybook": "^8.6.12",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=20.0.0 <=22.14.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "extensions.allowed": {
    "iconcaral2": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
